            string[] weekDays = new string[7];
            weekDays[0] = "Monday";
            weekDays[1] = "Tuesday";
            weekDays[2] = "Wednesday";
            weekDays[3] = "Thursday";
            weekDays[4] = "Friday";
            weekDays[5] = "Saturday";
            weekDays[6] = "Sunday";

            int day = int.Parse(Console.ReadLine());

            if(day < 1 || day > 7)
            {
                Console.WriteLine("Invalid day!");
            }
            else
            {
                Console.WriteLine(weekDays[day - 1]);
            }
        }
    }
}
