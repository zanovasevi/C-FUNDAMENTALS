using System;

namespace _09.Chars_to_String
{
    class Program
    {
        static void Main(string[] args)
        {
            char a = char.Parse(Console.ReadLine());
            char b = char.Parse(Console.ReadLine());
            char c = char.Parse(Console.ReadLine());

            string A = a.ToString();
            string B = b.ToString();
            string C = c.ToString();
            string comb = A + B + C;
            Console.WriteLine(comb);
        }
    }
}
