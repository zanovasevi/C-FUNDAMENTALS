using System;

namespace _10.Lower_or_Upper
{
    class Program
    {
        static void Main(string[] args)
        {
            char ch = char.Parse(Console.ReadLine());

            if(ch >= 60 && ch <= 90)
            {
                Console.WriteLine("upper-case");
            }
            if(ch >= 97 && ch <= 122)
            {
                Console.WriteLine("lower-case");
            }
        }
    }
}
